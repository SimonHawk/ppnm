set term svg background rgb "white" font ",16" size 600,800
set out "C.ODE.svg"

set grid

set multiplot layout 3,2 columnsfirst

set xrange [0:2*pi]
plot sin(x) with lines title "sin(x)" \
	, "out.dataC.txt" u 1:2 with lines title "y"

plot cos(x) with lines title "cos(x)" \
	, "out.dataC.txt" u 1:3 with lines title "y'"

plot -sin(x) with lines title "-sin(x)" \
	, "out.dataC.txt" u 1:4 with lines title "y''"

plot "out.dataC.txt" u 1:($2+$4)**2 with lines title "(y + y'')^2"
